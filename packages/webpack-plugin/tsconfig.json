{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "./lib/runtime/components/react/dist",
    "rootDir": "./lib/runtime/components/react",
    "allowJs": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ]
  }
}