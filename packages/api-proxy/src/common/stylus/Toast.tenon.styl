
.__mpx_toast__
  .__mpx_mask__
    z-index 1000
    top 0
    right 0
    left 0
    bottom 0
    display none
    &.show
      display block
  .__mpx_toast_box__
    // position: absolute
    // top 50%
    // left 50%
    z-index 2000
    box-sizing border-box
    display flex
    flex-direction column
    justify-content center
    width 200hm
    overflow hidden
    word-break normal
    padding 30hm 12hm
    border-radius 10hm
    background rgba(20, 20, 20, 0.7)
    .__mpx_toast_icon__
      width 80hm
      height 80hm
      background-size 100%
      background-color rgba(0,0,0,0)
      background-repeat no-repeat
      background-position-y center
      margin 12hm auto

    .__mpx_toast_title__
      text-align center
      line-height 1.6
      font-size 24hm
      color #fff
      margin 0
      overflow hidden
      max-width 100%
      text-overflow ellipsis
      display -webkit-box
      -webkit-line-clamp 2
      -webkit-box-orient vertical

.__hide_icon
  display none
  height 0
  margin 0
.__success_icon
  background-image url("data:image/png;base64,")
.__error_icon
  background-image url("data:image/png;base64,")
